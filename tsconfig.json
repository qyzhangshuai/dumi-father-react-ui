{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "dumi-react-ui/esm/*": [
        "src/*"
      ],
      "dumi-react-ui/lib/*": [
        "src/*"
      ],
      "dumi-react-ui": [
        "src/index.ts"
      ],
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src",
    "typings.d.ts"
  ],
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "dist",
    "typings",
    "**/__test__",
    "test"
  ]
}