{
  "name": "z-react-ui",
  "version": "1.0.2",
  "description": "z-react-ui，是一款基于 Dumi，由 React + TypeScript 开发的组件库 🎉。",
  "keywords": [
    "react",
    "typescript",
    "antd",
    "ui",
    "dumi",
    "father"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "rimraf docs-dist && npm run docs:build && npm run docs:deploy",
    
    "build": "npm run clean && father-build && gulp",
    "release": "npm run build && npm publish",
    "version": "node ./scripts/release.js",
    "v-release": "npm run version && npm run release",
    "clean": "rimraf lib es dist",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.esm.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "homepage": "https://qyzhangshuai.github.io/dumi-father-react-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qyzhangshuai/dumi-father-react-ui.git"
  },
  "author": "zhangshuai",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@babel/runtime": "^7.12.5",
    "antd": "^4.9.1",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.0.10",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssnano": "^2.1.3",
    "inquirer": "^7.3.3",
    "klaw-sync": "^6.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "ts-node": "^9.1.0",
    "vfile-message": "^2.0.4",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}